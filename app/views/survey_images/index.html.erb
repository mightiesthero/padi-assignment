<p id="notice"><%= notice %></p>

<h1>Survey Images</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Img</th>
      <th>Description</th>
      <th>Survey site</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_images.each do |survey_image| %>
      <tr>
        <td><%= survey_image.name %></td>
        <td><%= survey_image.img %></td>
        <td><%= survey_image.description %></td>
        <td><%= survey_image.survey_site %></td>
        <td><%= link_to 'Show', survey_image %></td>
        <td><%= link_to 'Edit', edit_survey_image_path(survey_image) %></td>
        <td><%= link_to 'Destroy', survey_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey Image', new_survey_image_path %>
